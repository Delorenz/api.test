<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Student" table="student">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prenomstudent" type="string" column="prenomStudent" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="nomstudent" type="string" column="nomStudent" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="emailstudent" type="string" column="emailStudent" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="promostudent" type="string" column="promoStudent" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="anneeacademiquestudent" type="integer" column="anneeAcademiqueStudent" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="genderstudent" type="string" column="genderStudent" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="campusstudent" type="string" column="campusStudent" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etatdossierstudent" type="string" column="etatDossierStudent" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
